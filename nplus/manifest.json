{
  "name": "NotionPlus",
  "version": "1.9.11",
  "description": "Programming in Notion.so",
  "permissions": [
    "activeTab",
    "storage",
    "http://127.0.0.1/"
  ],
  "__firefox__browser_specific_settings": {
    "gecko": {
      "id": "i@gine.me"
    }
  },
  "__chrome__options_page": "options.html",
  "__firefox__options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "/static/js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.notion.so/*"
      ],
      "js": [
        "/static/js/content-script-plus.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "logo/img_0628_16.png",
    "32": "logo/img_0628_32.png",
    "48": "logo/img_0628_48.png",
    "128": "logo/img_0628_128.png"
  },
  "manifest_version": 2
}