  const handleUserKeyPress = useCallback((e: React.KeyboardEvent) => {
    if (e.key === "i" && e.metaKey) {
      setOpen(!open);
    }
    // e.stopPropagation()
    // e.preventDefault();
  }, [open])

  useEffect(() => {
    window.addEventListener("keydown", handleUserKeyPress)
    return () => {
      window.removeEventListener('keydownk', handleUserKeyPress)
    };
  }, [handleUserKeyPress])